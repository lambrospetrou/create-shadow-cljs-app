{
  "name": "create-shadow-cljs-app",
  "version": "1.0.2",
  "description": "Easy creation of ClojureScript applications using Shadow-CLJS",
  "bin": {
    "create-shadow-cljs-app": "dist/index.js"
  },
  "main": "dist/index.js",
