{
  "name": "create-shadow-cljs-app",
  "version": "1.0.3",
  "description": "Easy creation of ClojureScript applications using Shadow-CLJS",
  "bin": {
    "create-shadow-cljs-app": "dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "watch:app": "shadow-cljs watch app",
    "release:app": "shadow-cljs release app",
    "postrelease:app": "shx cp -rf templates dist/",
    "prerelease": "shadow-cljs release library",
    "release": "rollup -c rollup.config.js",
    "postrelease": "shx cp -rf templates dist/"
  },
  "keywords": [
    "shadow-cljs",
    "clojurescript",
    "cljs",
    "node-script",
    "browser",
    "commonjs",
    "clojure"
  ],
  "author": "lambrospetrou",
  "devDependencies": {
    "colors": "^1.3.3",
    "rollup": "^1.20.2",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "shadow-cljs": "^2.8.51",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "yargs": "^14.0.0"
  },
  "dependencies": {},
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/lambrospetrou/create-shadow-cljs-app",
  "bugs": "https://github.com/lambrospetrou/create-shadow-cljs-app/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lambrospetrou/create-shadow-cljs-app.git"
  }
}
